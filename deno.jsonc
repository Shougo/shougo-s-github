{
  "lock": false,
  "tasks": {
    "cache": "deno install --reload",
    "check": "deno check vim/rc/*.ts",
    "lint": "deno lint vim/rc",
    "lint-fix": "deno lint --fix vim/rc",
    "fmt": "deno fmt vim/rc",
    "test": "deno test -A --doc --parallel --shuffle vim/rc/**/*.ts",
    "update": "deno outdated --recursive",
    "upgrade": "deno outdated --recursive --update --latest"
  },
  "workspace": [
    "./vim/rc"
  ]
}
